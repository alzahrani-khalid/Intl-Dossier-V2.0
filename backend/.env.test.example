# Test Environment Variables Template
# Copy this file to .env.test and fill in your actual values
# NEVER commit .env.test to version control

# Supabase Configuration
# Get these from: https://supabase.com/dashboard/project/YOUR_PROJECT_ID/settings/api
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Redis Configuration (optional for tests)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# AnythingLLM Configuration (optional for AI tests)
ANYTHINGLLM_API_URL=http://localhost:3001
ANYTHINGLLM_API_KEY=your-api-key-here
ANYTHINGLLM_WORKSPACE_SLUG=default
