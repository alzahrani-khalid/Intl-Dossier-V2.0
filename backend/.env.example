# Server Configuration
NODE_ENV=development
PORT=5000

# Frontend URL for CORS
FRONTEND_URL=http://localhost:3000

# Additional allowed origins (comma-separated)
ALLOWED_ORIGINS=

# API Security
API_KEYS=your-api-key-here,another-api-key

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key

# Database Configuration (if using direct connection)
DATABASE_URL=postgresql://username:password@localhost:5432/database

# Logging Configuration
LOG_LEVEL=debug

# IP Whitelist for admin endpoints (comma-separated)
ADMIN_IP_WHITELIST=127.0.0.1,::1

# JWT Configuration (if using custom auth)
JWT_SECRET=your-jwt-secret-here
JWT_EXPIRES_IN=24h

# File Upload Configuration
MAX_FILE_SIZE=10mb
UPLOAD_DIR=uploads

# Email Configuration (if using email notifications)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password

# Redis Configuration (for session storage/caching)
REDIS_URL=redis://localhost:6379

# AnythingLLM Configuration
ANYTHING_LLM_API_URL=http://localhost:3001/api
ANYTHING_LLM_API_KEY=your-anythingllm-api-key

# AI Provider Keys
OPENAI_API_KEY=sk-your-openai-key
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key
GOOGLE_AI_API_KEY=your-google-ai-key

# Private LLM Configuration (optional)
VLLM_BASE_URL=http://localhost:8000
OLLAMA_BASE_URL=http://localhost:11434

# AI Feature Flags
AI_BRIEF_GENERATION_ENABLED=true
AI_CHAT_ENABLED=true
AI_ENTITY_LINKING_ENABLED=true
AI_USE_ANYTHINGLLM=false

# AI Embeddings
AI_EMBEDDING_MODEL=bge-m3
AI_EMBEDDING_DIMENSIONS=1024

# Sentry Error Tracking (Optional - leave empty to disable)
# Get these from https://sentry.io/settings/projects/
SENTRY_DSN=
SENTRY_ENVIRONMENT=development
SENTRY_RELEASE=