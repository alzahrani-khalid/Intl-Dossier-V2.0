# Supabase Configuration
SUPABASE_URL=http://localhost:54321
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-key-here
DATABASE_URL=postgresql://postgres:postgres@localhost:54322/postgres

# AnythingLLM Configuration
ANYTHINGLLM_API_URL=http://localhost:3001
ANYTHINGLLM_API_KEY=your-anythingllm-key-here

# AI Model Configuration
EMBEDDING_MODEL=bge-m3
EMBEDDING_DIM=1024
SIMILARITY_PRIMARY=0.82
SIMILARITY_CANDIDATE=0.65

# SLA Configuration (in minutes)
HIGH_PRIORITY_ACK_MINUTES=30
HIGH_PRIORITY_RESOLVE_MINUTES=480
MEDIUM_PRIORITY_ACK_MINUTES=120
MEDIUM_PRIORITY_RESOLVE_MINUTES=1440
LOW_PRIORITY_ACK_MINUTES=480
LOW_PRIORITY_RESOLVE_MINUTES=2880

# Storage Configuration
MAX_FILE_SIZE_MB=25
MAX_TOTAL_SIZE_MB=100
STORAGE_BUCKET=intake-attachments

# Rate Limiting
USER_RATE_LIMIT=300
ANON_RATE_LIMIT=60
AI_RATE_LIMIT=60
GLOBAL_RATE_LIMIT=15000

# Application Settings
NODE_ENV=development
PORT=3000
API_PORT=54321
FRONTEND_URL=http://localhost:3000
API_URL=http://localhost:54321

# Security
JWT_SECRET=your-jwt-secret-here
ENCRYPTION_KEY=your-encryption-key-here
MFA_ISSUER=GASTAT-Intake

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090
LOG_LEVEL=info

# Business Hours (for SLA calculations)
BUSINESS_HOURS_START=08:00
BUSINESS_HOURS_END=17:00
BUSINESS_TIMEZONE=Asia/Riyadh

# Data Retention (in days)
ACTIVE_RETENTION_DAYS=90
ARCHIVE_RETENTION_DAYS=1095
AUDIT_RETENTION_DAYS=2555

# Feature Flags
ENABLE_AI_TRIAGE=true
ENABLE_DUPLICATE_DETECTION=true
ENABLE_AUTO_ASSIGNMENT=true
AI_FALLBACK_ENABLED=true

# Localization
DEFAULT_LOCALE=en
SUPPORTED_LOCALES=en,ar
RTL_LOCALES=ar